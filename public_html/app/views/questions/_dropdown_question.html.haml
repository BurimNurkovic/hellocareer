- q = dropdown_question

.question-content
  .grid-col-5.grid-col__left.col-space__right
    %label
      = q.builder_text
    - unless q.tip_text.blank?
      .question-tip
        .open__button
        .tip__text
          %p
            = unescape_newline q.tip_text
  .grid-col-7.grid-col__left
    - f = locals[:form_builder]
    = f.select dropdown_question.key, q.special_data, include_blank: true


