- q = autocomplete_question
.question-content
  .grid-col-5.grid-col__left.col-space__right
    %label
      = q.builder_text
    - unless q.tip_text.blank?
      .question-tip
        .open__button
        .tip__text
          %p
            = unescape_newline q.tip_text
  .grid-col-7.grid-col__left.autocomplete-question{:data => {:qid => q.id, :revision => q.updated_at.to_i}}
    = locals[:form_builder].text_field q.key
