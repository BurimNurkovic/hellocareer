/ #cv-design
/   %p
/     Views: 
/     %strong= @resume.views_count
/   %page.main-format
/     .content.color-blue
/       - unless @resume.short_description.about_surname_key.blank? && @resume.short_description.about_name_key.blank?
/         %h1
/           = @resume.short_description.about_surname_key
/           = @resume.short_description.about_name_key

/       - unless @resume.position.pos_position_key.blank?
/         %h3
/           = @resume.position.pos_position_key

/       - unless @resume.short_description.about_email_key.blank?
/         %p
/           EMAIL:
/           = @resume.short_description.about_email_key

/       - unless @resume.short_description.about_blog_key.blank?
/         %p
/           WEBSITE:
/           = @resume.short_description.about_blog_key

/       - unless @resume.short_description.about_tel_key.blank?
/         %p
/           TELEFON:
/           = @resume.short_description.about_tel_key

/       - unless @resume.short_description.about_live_key.blank?
/         %p
/           ADRESA:
/           = @resume.short_description.about_live_key

/     - unless @resume.short_description.about_summary_key.blank?
/       .content
/         %h4 Despre mine
/         %p
/           = @resume.short_description.about_summary_key

/     - unless @resume.jobs.blank?
/       .content
/         %h4 Experiența de lucru

/         - @resume.jobs.each do |answer_set|
/           .col-content
/             .col-5
/               %p
/                 = answer_set.job_start_key.strftime("%d/%m/%Y")
/                 %span -
/                 - if answer_set.job_finish_key.blank?
/                   Prezent
/                 - else
/                   = answer_set.job_finish_key.strftime("%d/%m/%Y")
/               %p
/                 - unless answer_set.job_time_key.blank?
/                   = answer_set.job_time_key
/             .col-13
/               %h5
/                 = answer_set.job_post_key
/               %h5
/                 = answer_set.job_cpy_key
/                 ,
/                 = answer_set.job_cpy_loc_key
/               %p
/                 Activități de bază:
/                 = answer_set.job_respon_key
/               %p
/                 Realizări:
/                 = answer_set.job_realiz_key

/     - unless @resume.internships.blank?
/       .content
/         %h4 Stagii

/         - @resume.internships.each do |answer_set|
/           .col-content
/             .col-5
/               %p
/                 = answer_set.start_start_key.strftime("%d/%m/%Y")
/                 %span -
/                 - if answer_set.stag_finish_key.blank?
/                   Prezent
/                 - else
/                   = answer_set.stag_finish_key.strftime("%d/%m/%Y")
/               %p
/                 - unless answer_set.stag_time_key.blank?
/                   = answer_set.stag_time_key
/             .col-13
/               %h5
/                 = answer_set.stag_position_key
/               %h5
/                 = answer_set.stag_cpy_key
/                 ,
/                 = answer_set.stag_cpy_loc_key
/               %p
/                 Activități de bază:
/                 = answer_set.stag_activ_key
/               %p
/                 Realizări:
/                 = answer_set.stag_aport_key

/     - unless @resume.volunteerings.blank?
/       .content
/         %h4 Voluntariat

/         - @resume.volunteerings.each do |answer_set|
/           .col-content
/             .col-5
/               %p
/                 = answer_set.volunt_start_key.strftime("%d/%m/%Y")
/                 %span -
/                 - if answer_set.volunt_finish_key.blank?
/                   Prezent
/                 - else
/                   = answer_set.volunt_finish_key.strftime("%d/%m/%Y")
/               %p
/                 - unless answer_set.volunt_time_key.blank?
/                   = answer_set.volunt_time_key
/             .col-13
/               %h5
/                 - if answer_set.volunt_role_key == 'Organizator'
/                   = answer_set.volunt_org_key
/                 - else
/                   = answer_set.volunt_role_key
/               %h5
/                 = answer_set.volunt_name_key
/               %p
/                 Activități de bază:
/                 = answer_set.volunt_resp_key
/               %p
/                 Realizări:
/                 = answer_set.volunt_realiz_key

